  <div class="contents row" >
    <p><%= @nickname %>さんの投稿一覧</p>
    <% @tweets.each do |tweet| %>
      <div class="content_post" style="background-image: url(<%= tweet.image %>);">
        <%= simple_format(tweet.text) %>
        <span class="name">
          <span>投稿者</span><%= current_user.nickname %>
        </span>
      </div>
    <% end %>
    <%= paginate(@tweets) %>
  </div>